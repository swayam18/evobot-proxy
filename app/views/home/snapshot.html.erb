<div class="container-fluid">
  <div class="row">
    <%= render partial: "sidebar", locals: { page: "snapshot" } %>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
      <div class="page-header">
        <h1> Camera Snapshot </h1>
      </div>
      <div id="other-container" style="min-width: 310px; height: 400px; max-width: 700px; margin: 20px auto">
        <%= image_tag @snapshot.url %>
        <p><%="Taken on: #{Time.at(@snapshot.updated_at).to_formatted_s :time}"%> </p>
      </div>
    </div>
  </div>
</div>
